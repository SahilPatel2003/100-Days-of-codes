#include<iostream>
using namespace std;
int main()
{
   int n;//enter the size of array
   cin>>n;

   int arr[n];

   for(int i=0;i<n;i++)
   {
     cin>>arr[i]; //enter the elements of array // 1 2 3 4 5
   }

   int max=*max_element(arr,arr+n); //maximum elemennt of array
   int min=*min_element(arr,arr+n); //minimum element of array
   cout<<min<<" "<<max; // 1 5
   return 0;
}
