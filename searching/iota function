#include<iostream>
#include<numeric> 
using namespace std;
int main()
{
   int n;//enter the size of array
   cin>>n; // 5

   int arr[n];
   iota(arr,arr+n,1);// array start with 1 value then increament 1 while arr will not compelete
   for(int i=0;i<n;i++)
   {
     cout<<arr[i]; // 1 2 3 4 5
   }

   return 0;
}
